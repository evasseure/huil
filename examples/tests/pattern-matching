print("Type a number to translate (0 -> 9):")
let num = input("-> ")
let letter = match num:
    | 0 -> "zero"
    | 1 -> "one"
    | 2 -> "two"
    | 3 -> "three"
    | 4 -> "four"
    | 5 -> "five"
    | 6 -> "six"
    | 7 -> "seven"
    | 8 -> "height"
    | 9 -> "nine"
    | * -> "I don't know sorry"

