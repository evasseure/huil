fn index(req):
    "<html><h1>Hello world</h1></html>"

fn books(req):
    let books = ["book" + i for i in ..5]
    return books

fn listen(req):
    match req.url:
        | "/" -> index(req)
        | "/books" -> books(req)
        | * -> not_found(req)

let server = Server()
server.listen(8000, listen)
